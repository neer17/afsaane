{
  "name": "afsaane",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.17.0",
    "yarn": ">=1.22.19",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "rm -rf .next && next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:fix": "prettier -c .prettierrc --ignore-path prettier-ignore --write src",
    "prepare": "husky",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint '*/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --write */**/*.{js,jsx,json,ts,tsx,scss,css,md}"
  },
  "dependencies": {
    "firebase": "^11.1.0",
    "idb": "^8.0.1",
    "lint-staged": "^15.4.2",
    "next": "14.2.15",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.5",
    "swiper": "^11.1.14",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "^15.1.6",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "typescript": "^5",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3"
  }
